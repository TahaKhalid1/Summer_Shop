    <%--<link href="../UserTemplate/assets/css/theme.css" rel="stylesheet" />--%>

        <style>
            /* General Responsive Styles */
            body {
                margin: 0;
                padding: 0;
                position: relative;
                padding-top: 120px; /* Adjust based on your header height */
                font-family: 'Poppins', sans-serif;
                overflow-x: hidden;
            }

            /* Header Styles */
            header {
                position:fixed;
                top: 0;
                left: 0;
                width: 100%;
                z-index: 10000;
                background: #fff;
                box-shadow: 0 2px 15px rgba(0,0,0,0.1);
            }

           
            .wrap-menu-desktop {
                position: static;
            }


            
            
            

            /* Mobile menu style */
            .mobile-menu {
                position: fixed;
                top: 70px;
                left: 0;
                right: 0;
                bottom: 0;
                background: #fff;
                z-index: 9999;
                overflow-y: auto;
                padding: 20px;
                display: none;
                box-shadow: 0 5px 15px rgba(0,0,0,0.1);
            }
        
            .mobile-menu.active {
                display: block;
            }
        
            .mobile-menu > ul {
                list-style: none;
                padding: 0;
                margin: 0;
            }
        
            .mobile-menu > ul > li {
                position: relative;
                border-bottom: 1px solid #eee;
            }
        
            .mobile-menu > ul > li > a {
                color: #333;
                display: block;
                padding: 15px 0;
                font-weight: 500;
                text-decoration: none;
            }
        
            .mobile-mega-menu {
                display: none;
                background: #f9f9f9;
                padding: 15px;
                margin-top: 10px;
                border-left: 3px solid #717fe0;
            }
        
            .mobile-mega-menu.active {
                display: block;
                animation: fadeIn 0.3s ease;
            }
        
            .mobile-mega-col {
                margin-bottom: 20px;
            }
        
            .mobile-mega-col h4 {
                font-size: 14px;
                color: #333;
                margin-bottom: 10px;
                text-transform: uppercase;
                font-weight: 700;
                padding-bottom: 5px;
                border-bottom: 1px solid #eee;
            }
        
            .mobile-mega-col ul {
                list-style: none;
                padding: 0;
                margin: 0;
            }
        
            .mobile-mega-col ul li {
                margin-bottom: 8px;
            }
        
            .mobile-mega-col ul li a {
                color: #555;
                font-size: 13px;
                transition: all 0.2s ease;
                display: block;
                padding: 5px 0;
            }
        
            .mobile-mega-col ul li a:hover {
                color: #717fe0;
                padding-left: 8px;
            }
        
            .mobile-arrow {
                float: right;
                transition: transform 0.3s ease;
                margin-left: 10px;
            }
        
            .mobile-arrow.active {
                transform: rotate(90deg);
            }

            /* Account dropdown styles */
            .wrap-icon-header {
                display: flex;
                align-items: center;
                gap: 10px;
            }

            .icon-header-item {
                display: flex;
                align-items: center;
                justify-content: center;
                padding: 0 10px !important;
            }

            /* Account Dropdown Styles */
            .account-dropdown {
                position: relative;
                display: flex;
                align-items: center;
            }

            .account-info {
                display: flex;
                align-items: center;
                cursor: pointer;
                padding: 0 10px;
            }

            .login-label {
                margin-left: 8px;
                font-size: 14px;
                white-space: nowrap;
            }

            .account-dropdown-content {
                display: none;
                position: absolute;
                right: 0;
                top: 100%;
                background-color: #fff;
                min-width: 200px;
                box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
                z-index: 1;
                border-radius: 4px;
                padding: 10px 0;
            }

            .account-dropdown:hover .account-dropdown-content {
                display: block;
                animation: fadeIn 0.2s ease-out;
            }

            .account-dropdown-content a {
                color: #333;
                padding: 10px 16px;
                text-decoration: none;
                display: block;
                font-size: 14px;
            }

            .account-dropdown-content a:hover {
                background-color: #f5f5f5;
                color: #717fe0;
            }

            /* Ensure all icons are the same size */
            .icon-header-item i {
                font-size: 20px;
            }

            /* Mobile menu adjustments */
            @media (max-width: 991px) {
                .wrap-icon-header {
                    gap: 5px;
                }
            
                .icon-header-item {
                    padding: 0 5px !important;
                }
            
                .account-dropdown-content {
                    right: auto;
                    left: 0;
                    min-width: 180px;
                }
            }

            /* Cart styles */
            .wrap-header-cart {
                position: fixed;
                z-index: 1100;
                width: 100%;
                height: 100vh;
                top: 0;
                right: 0;
                background-color: rgba(0,0,0,0.0);
                visibility: hidden;
            
                -webkit-transition: all 0.4s;
                -o-transition: all 0.4s;
                -moz-transition: all 0.4s;
                transition: all 0.4s;
            }

            .header-cart {
                position: fixed;
                z-index: 1100;
                width: 390px;
                max-width: calc(100% - 30px);
                height: 100vh;
                top: 0;
                right: -400px;
                background-color: #fff;
            
                -webkit-transition: all 0.4s;
                -o-transition: all 0.4s;
                -moz-transition: all 0.4s;
                transition: all 0.4s;

                box-shadow: 0 3px 6px 0px rgba(0, 0, 0, 0.18);
            }

            .show-header-cart {
                visibility: visible;
                background-color: rgba(0,0,0,0.6);
            }

            .show-header-cart .header-cart {
                right: 0;
            }

            /* Footer responsive styles */
            @media (max-width: 991px) {
                .footer-column {
                    width: 33.33% !important;
                    margin-bottom: 30px;
                }
            }

            @media (max-width: 767px) {
                .footer-column {
                    width: 50% !important;
                }
            }

            @media (max-width: 575px) {
                .footer-column {
                    width: 100% !important;
                }
            
                .footer-content {
                    flex-direction: column;
                }
            }

            /* Responsive logo */
            .logo img {
                max-width: 100%;
                height: auto;
            }

            /* Hamburger menu icon */
            .hamburger {
                padding: 15px;
                cursor: pointer;
            }

            /* Search modal responsive */
            @media (max-width: 767px) {
                .wrap-search-header input,
                .wrap-search-header button {
                    font-size: 18px;
                }

                .wrap-search-header {
                    height: 60px;
                }
            }

            /* General responsive utilities */
            .hidden-mobile {
                display: block;
            }
        
            .hidden-desktop {
                display: none;
            }
        
            @media (max-width: 991px) {
                .hidden-mobile {
                    display: none !important;
                }
            
                .hidden-desktop {
                    display: block !important;
                }
            }

            /* Center the New Arrival menu item */
            
        </style>
    </head>
