            <!-- Vendor JS -->
            

            
            <script>
                // Mobile menu toggle
                $(document).ready(function () {
                    $('.btn-show-menu-mobile').on('click', function () {
                        $(this).toggleClass('is-active');
                        $('.mobile-menu').toggleClass('active');
                    });

                    // For touch devices - toggle mega menu on click
                    if (matchMedia('(hover: none) and (pointer: coarse)').matches) {
                        $('.mobile-menu > ul > li > a').on('click', function (e) {
                            if ($(this).siblings('.mobile-mega-menu').length) {
                                e.preventDefault();
                                $(this).find('.mobile-arrow').toggleClass('active');
                                $(this).siblings('.mobile-mega-menu').toggleClass('active');
                            }
                        });
                    }

                    // Account dropdown toggle
                    $('.account-info').on('click', function (e) {
                        e.stopPropagation();
                        $('.account-dropdown-content').toggle();
                    });

                    // Close dropdown when clicking outside
                    $(document).on('click', function () {
                        $('.account-dropdown-content').hide();
                    });

                    // Prevent dropdown from closing when clicking inside it
                    $('.account-dropdown-content').on('click', function (e) {
                        e.stopPropagation();
                    });

                    // Handle cart icon click for unauthenticated users
                    $('.header-icon2').on('click', function (e) {
                        // Check if user is logged in (you might need to adjust this condition)
                        if (typeof isUserLoggedIn === 'undefined' || !isUserLoggedIn) {
                            e.preventDefault();
                            swal({
                                title: "Oops!",
                                text: "You are logged out. Kindly log in to check your cart",
                                icon: "warning",
                                buttons: {
                                    login: {
                                        text: "Login",
                                        value: "login",
                                    },
                                    cancel: "Cancel"
                                }
                            }).then((value) => {
                                if (value === "login") {
                                    window.location.href = "Login.aspx?ReturnUrl=Cart.aspx";
                                }
                            });
                        }
                    });
                });

                // Select2 initialization
                $(".js-select2").each(function () {
                    $(this).select2({
                        minimumResultsForSearch: 20,
                        dropdownParent: $(this).next('.dropDownSelect2')
                    });
                });

                // Parallax initialization
                $('.parallax100').parallax100();

                // Lightbox initialization
                $('.gallery-lb').each(function () {
                    $(this).magnificPopup({
                        delegate: 'a',
                        type: 'image',
                        gallery: {
                            enabled: true
                        },
                        mainClass: 'mfp-fade'
                    });
                });

                // Perfect scrollbar initialization
                $('.js-pscroll').each(function () {
                    $(this).css('position', 'relative');
                    $(this).css('overflow', 'hidden');
                    var ps = new PerfectScrollbar(this, {
                        wheelSpeed: 1,
                        scrollingThreshold: 1000,
                        wheelPropagation: false,
                    });

                    $(window).on('resize', function () {
                        ps.update();
                    })
                });
            </script>
