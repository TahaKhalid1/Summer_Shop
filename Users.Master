            /* Mega Menu Styles */
	
		    body {
                margin: 0;
                padding: 0;
                position: relative;
                padding-top: 120px; /* Adjust based on your header height */
            }

            header {
                position: fixed;
                top: 0;
                left: 0;
                width: 100%;
                z-index: 10000;
                background: #fff;
                box-shadow: 0 2px 15px rgba(0,0,0,0.1);
            }

            .container-menu-desktop {
                position: relative;
                z-index: 10001;
            }

            .wrap-menu-desktop {
                position: static;
            }

            /* Main content area - pushes content below fixed header */
            main {
                position: relative;
                z-index: 1;
                margin-top: 80px; /* Adjust based on your header height */
            }

            /* Mobile header adjustments */
            @media (max-width: 991px) {
                body {
                    padding-top: 60px; /* Smaller header on mobile */
                }
            
                header {
                    position: fixed;
                }
            
                .wrap-header-mobile {
                    position: relative;
                    z-index: 10002;
                }
            }
		    .mega-menu {
			    position: absolute;
			    left: 0;
			    width: 100%;
			    background: #fff;
			    padding: 30px 20px;
			    box-shadow: 0 5px 25px rgba(0,0,0,0.1);
			    z-index: 1000;
			    display: none;
			    border-top: 2px solid #717fe0;
			    margin-top: -1px;
		    }

		    .main-menu > li:hover .mega-menu {
			    display: flex;
			    animation: fadeIn 0.15s ease-out;
		    }

		    .mega-menu-col {
			    flex: 1;
			    padding: 0 15px;
			    min-width: 200px;
			    border-right: 1px solid #f0f0f0; /* Subtle column separator */
		    }

		    .mega-menu-col:last-child {
			    border-right: none; /* Remove border from last column */
		    }

		    .mega-menu h4 {
			    font-size: 14px;
			    color: #333;
			    margin-bottom: 15px;
			    text-transform: uppercase;
			    font-weight: 700; /* Bolder font weight */
			    position: relative;
			    padding-bottom: 8px;
			    letter-spacing: 0.5px; /* Better readability */
		    }

		    .mega-menu h4:after {
			    content: '';
			    position: absolute;
			    left: 0;
			    bottom: 0;
			    width: 40px; /* Longer underline */
			    height: 2px;
			    background: #717fe0;
		    }

		    .mega-menu ul {
			    list-style: none;
			    padding: 0;
			    margin: 0 0 25px 0; /* More space between sections */
		    }

		    .mega-menu ul li {
			    margin-bottom: 10px; /* Increased spacing between items */
		    }

		    .mega-menu ul li a {
			    color: #555;
			    font-size: 13px;
			    transition: all 0.2s ease;
			    display: block;
			    padding: 5px 0;
			    line-height: 1.4;
		    }

		    .mega-menu ul li a:hover {
			    color: #717fe0;
			    padding-left: 8px; /* More noticeable hover effect */
			    font-weight: 500;
		    }

		    .mega-menu-header {
			    font-size: 18px;
			    font-weight: 700;
			    color: #333;
			    margin-bottom: 25px;
			    text-transform: uppercase;
			    border-bottom: 2px solid #717fe0;
			    padding-bottom: 10px;
			    display: block;
			    letter-spacing: 1px;
		    }

		    /* Animation for smooth appearance */
		    @keyframes fadeIn {
			    from { 
				    opacity: 0; 
				    transform: translateY(8px); 
			    }
			    to { 
				    opacity: 1; 
				    transform: translateY(0); 
			    }
		    }

		    /* Adjust main menu items */
		    .main-menu > li {
			    position: static; /* Changed from relative for better dropdown positioning */
		    }

		    .main-menu > li > a {
			    padding: 22px 18px; /* Slightly larger click area */
			    display: inline-block;
			    font-weight: 500;
			    position: relative;
			    color: #333;
			    transition: all 0.2s;
		    }

		    .main-menu > li > a:after {
			    content: '';
			    position: absolute;
			    left: 18px;
			    bottom: 18px;
			    width: 0;
			    height: 2px;
			    background: #717fe0;
			    transition: width 0.3s ease-out;
		    }

		    .main-menu > li:hover > a {
			    color: #717fe0;
		    }

		    .main-menu > li:hover > a:after {
			    width: calc(100% - 36px);
		    }

		    /* Container adjustments to prevent clipping */
		    .wrap-menu-desktop {
			    position: relative;
			    z-index: 1001;
		    }

		    .container-menu-desktop {
			    overflow: visible !important;
		    }

		    /* Mobile adjustments */
		    @media (max-width: 991px) {
			    .mega-menu {
				    position: static;
				    display: none;
				    box-shadow: none;
				    padding: 15px;
				    background: #f9f9f9;
			    }
        
			    .mega-menu-col {
				    border-right: none;
				    margin-bottom: 20px;
			    }
        
			    .main-menu > li > a:after {
				    display: none;
			    }
		    }

		    /* Fallbacks for older browsers */
		    @supports not (backdrop-filter: blur(10px)) {
			    .mega-menu {
				    background: #fff;
			    }
		    }
            /* Add this new mobile menu style */
            .mobile-menu {
                position: fixed;
                top: 120px; /* Adjusted based on your header height */
                left: 0;
                right: 0;
                bottom: 0;
                background: #fff;
                z-index: 9999;
                overflow-y: auto;
                padding: 20px;
                display: none;
                box-shadow: 0 5px 15px rgba(0,0,0,0.1);
            }
        
            .mobile-menu.active {
                display: block;
            }
        
            .mobile-menu > ul {
                list-style: none;
                padding: 0;
                margin: 0;
            }
        
            .mobile-menu > ul > li {
                position: relative;
                border-bottom: 1px solid #eee;
            }
        
            .mobile-menu > ul > li > a {
                color: #333;
                display: block;
                padding: 15px 0;
                font-weight: 500;
                text-decoration: none;
            }
        
            .mobile-mega-menu {
                display: none;
                background: #f9f9f9;
                padding: 15px;
                margin-top: 10px;
                border-left: 3px solid #717fe0;
            }
        
            .mobile-mega-menu.active {
                display: block;
                animation: fadeIn 0.3s ease;
            }
        
            .mobile-mega-col {
                margin-bottom: 20px;
            }
        
            .mobile-mega-col h4 {
                font-size: 14px;
                color: #333;
                margin-bottom: 10px;
                text-transform: uppercase;
                font-weight: 700;
                padding-bottom: 5px;
                border-bottom: 1px solid #eee;
            }
        
            .mobile-mega-col ul {
                list-style: none;
                padding: 0;
                margin: 0;
            }
        
            .mobile-mega-col ul li {
                margin-bottom: 8px;
            }
        
            .mobile-mega-col ul li a {
                color: #555;
                font-size: 13px;
                transition: all 0.2s ease;
                display: block;
                padding: 5px 0;
            }
        
            .mobile-mega-col ul li a:hover {
                color: #717fe0;
                padding-left: 8px;
            }
        
            .mobile-arrow {
                float: right;
                transition: transform 0.3s ease;
                margin-left: 10px;
            }
        
            .mobile-arrow.active {
                transform: rotate(90deg);
            }
        
            /* Add hover effect for mobile menu items */
            @media (hover: hover) and (pointer: fine) {
                .mobile-menu > ul > li:hover > .mobile-mega-menu {
                    display: block;
                }
            
                .mobile-menu > ul > li:hover > a .mobile-arrow {
                    transform: rotate(90deg);
                }
            }
        
            /* For touch devices */
            @media (hover: none) and (pointer: coarse) {
                .mobile-menu > ul > li > a {
                    position: relative;
                    padding-right: 30px;
                }
            }

            @keyframes fadeIn {
                from { opacity: 0; transform: translateY(10px); }
                to { opacity: 1; transform: translateY(0); }
            }

