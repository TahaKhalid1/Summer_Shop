                        // Toggle body overflow when menu is open
                        if ($('.mobile-menu').hasClass('active')) {
                            $('body').css('overflow', 'hidden');
                        } else {
                            $('body').css('overflow', 'auto');
                        }
                    });

                    // Mobile submenu toggle
                    $('.mobile-menu > ul > li.has-submenu > a').on('click', function (e) {
                        if ($(window).width() <= 991) {
                            e.preventDefault();
                            var $parent = $(this).parent();
                            $parent.toggleClass('active');
                            $parent.find('> .sub-menu-m').slideToggle(300);
                        }
                    });

                    // Close mobile menu when clicking outside
                    $(document).on('click', function (e) {
                        if (!$(e.target).closest('.mobile-menu, .btn-show-menu-mobile').length) {
                            $('.mobile-menu').removeClass('active');
                            $('.btn-show-menu-mobile').removeClass('is-active');
                            $('body').css('overflow', 'auto');
                        }
                    });

                    // Account dropdown toggle
                    $('.account-info').on('click', function (e) {
                        e.stopPropagation();
                        $(this).closest('.account-dropdown').find('.account-dropdown-content').toggle();
