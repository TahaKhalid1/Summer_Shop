            <script>

                $(document).ready(function () {
                    // Mobile menu toggle
                    $('.btn-show-menu-mobile').on('click', function () {
                        $(this).toggleClass('is-active');
                        $('.mobile-menu').toggleClass('active');

                        // Toggle body overflow when menu is open
                        if ($('.mobile-menu').hasClass('active')) {
                            $('body').css('overflow', 'hidden');
                        } else {
                            $('body').css('overflow', 'auto');
                        }
                    });

                    // Mobile submenu toggle
                    $('.mobile-menu > ul > li.has-submenu > a').on('click', function (e) {
                        if ($(window).width() <= 991) {
                            e.preventDefault();
                            var $parent = $(this).parent();
                            $parent.toggleClass('active');
                            $parent.find('> .sub-menu-m').slideToggle(300);
                        }
                    });

                    // Close mobile menu when clicking outside
                    $(document).on('click', function (e) {
                        if (!$(e.target).closest('.mobile-menu, .btn-show-menu-mobile').length) {
                            $('.mobile-menu').removeClass('active');
                            $('.btn-show-menu-mobile').removeClass('is-active');
                            $('body').css('overflow', 'auto');
                        }
                    });

                    // Account dropdown toggle
                    $('.account-info').on('click', function (e) {
                        e.stopPropagation();
                        $(this).closest('.account-dropdown').find('.account-dropdown-content').toggle();
                    });

                    // Close dropdown when clicking outside
                    $(document).on('click', function () {
                        $('.account-dropdown-content').hide();
                    });

                    // Prevent dropdown from closing when clicking inside it
                    $('.account-dropdown-content').on('click', function (e) {
                        e.stopPropagation();
                    });

                    // Handle cart icon click for unauthenticated users
                    //$('.js-show-cart').on('click', function(e) {
                    //    // Check if user is logged in (you might need to adjust this condition)
                    //    if (typeof isUserLoggedIn === 'undefined' || !isUserLoggedIn) {
                    //        e.preventDefault();
                    //        swal({
                    //            title: "Oops!",
                    //            text: "You are logged out. Kindly log in to check your cart",
                    //            icon: "warning",
                    //            buttons: {
                    //                login: {
                    //                    text: "Login",
                    //                    value: "login",
                    //                },
                    //                cancel: "Cancel"
                    //            }
                    //        }).then((value) => {
                    //            if (value === "login") {
                    //                window.location.href = "Login.aspx?ReturnUrl=Cart.aspx";
                    //            }
                    //        });
                    //    }
                    //});

                    // Initialize submenu classes for mobile
                    $('.mobile-menu > ul > li').each(function () {
                        if ($(this).find('> .sub-menu-m').length) {
                            $(this).addClass('has-submenu');
                        }
                    });
                });

                // Select2 initialization
                $(".js-select2").each(function () {
                    $(this).select2({
                        minimumResultsForSearch: 20,
                        dropdownParent: $(this).next('.dropDownSelect2')
                    });
                });

                // Parallax initialization
                $('.parallax100').parallax100();

                // Lightbox initialization
                $('.gallery-lb').each(function () {
                    $(this).magnificPopup({
                        delegate: 'a',
                        type: 'image',
                        gallery: {
                            enabled: true
                        },
                        mainClass: 'mfp-fade'
                    });
                });

                // Perfect scrollbar initialization
                $('.js-pscroll').each(function () {
                    $(this).css('position', 'relative');
                    $(this).css('overflow', 'hidden');
                    var ps = new PerfectScrollbar(this, {
                        wheelSpeed: 1,
                        scrollingThreshold: 1000,
                        wheelPropagation: false,
                    });

                    $(window).on('resize', function () {
                        ps.update();
                    })
                });
            </script>
